Host *
  AddKeysToAgent yes
  UseKeychain yes
  ForwardAgent no
  ForwardX11 no
  ForwardX11Trusted yes
# User root
  Port 22
  Protocol 2
  ServerAliveInterval 60
  ServerAliveCountMax 30
  IdentityFile ~/.ssh/id_rsa

